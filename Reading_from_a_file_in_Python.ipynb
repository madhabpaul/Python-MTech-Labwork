      "source": [
        "# similar syntax as you've seen, 'r' for reading. You can just throw a .read() at\n",
        "# the end, and you get:\n",
        "readMe = open('exampleFile.txt','r').read()\n",
        "print(readMe)\n",
        "# this will instead read the file into a python list.\n",
        "readMe = open('exampleFile.txt','r').readlines()\n",
        "print(readMe)"
      ]
